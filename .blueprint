models:
    User: { name: string, email: string, email_verified_at: 'timestamp nullable', password: string, remember_token: 'string:100 nullable' }
    buy: { name: 'string nullable', email: 'string nullable', ticket: 'string:15' }
    Conference: { name: string, description: string, start_date: datetime, end_date: datetime, status: string, region: string, venue_id: 'unsignedInteger nullable', relationships: { belongsTo: Venue, belongsToMany: 'Speaker, Talk' } }
    Venue: { name: string, city: string, country: string, postal_code: string, relationships: { hasMany: Conference } }
    Speaker: { name: string, email: string, bio: text, twitter_handle: text, relationships: { hasMany: Talk } }
    Talk: { title: string, abstract: text, speaker_id: unsignedInteger, relationships: { belongsTo: Venue, belongsToMany: Conference } }
    Ticket: { buyer: string, description: string, type: text, price: float, status: string, relationships: { belongsTo: Conference } }
